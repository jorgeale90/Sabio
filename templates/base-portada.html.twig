<script src="{{ asset('page/portada/public/vendors/is/is.min.js') }}"></script>
<script src="https://polyfill.io/v3/polyfill.min.js?features=window.scroll"></script>
<script src="{{ asset('page/portada/public/vendors/feather-icons/feather.min.js') }}"></script>
<script>
    feather.replace();

    const buttonTop = document.querySelector('#buttonTop');

    window.onscroll = () => {
        if (document.documentElement.scrollTop > 100) {
            buttonTop.classList.add('shows')
        } else {
            buttonTop.classList.remove('shows')
        }
        buttonTop.addEventListener('click', () => {
            window.scrollTo({
                top: 0,
                behavior: 'smooth'
            })
        })
    }
</script>
<script src="{{ asset('page/portada/public/assets/js/theme.js') }}"></script>
